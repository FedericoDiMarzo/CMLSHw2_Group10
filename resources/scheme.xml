<?xml version="1.0" encoding="UTF-8"?>

<magic>
  <Styles>
    <Style name="default">
      <Nodes/>
      <Classes>
        <plot-view border="2" background-color="black" border-color="silver" display="contents"/>
        <nomargin margin="0" padding="0" border="0"/>
        <group margin="5" padding="5" border="2" flex-direction="column"/>
      </Classes>
      <Types>
        <Slider border="0" slider-textbox="textbox-below"/>
        <ToggleButton border="0" max-height="50" caption-size="0" text="Active"/>
        <TextButton border="0" max-height="50" caption-size="0"/>
        <ComboBox border="0" max-height="50" caption-size="0"/>
        <Plot border="0" margin="0" padding="0" background-color="00000000"
              radius="0"/>
        <XYDragComponent border="0" margin="0" padding="0" background-color="00000000"
                         radius="0"/>
      </Types>
    </Style>
  </Styles>
  <View resizable="0" resize-corner="0" width="600" height="400" background-image="background_jpg"
        image-placement="centred" background-color="FF2A2D37" background-alpha="1"
        margin="0" padding="0" border="0" radius="0" border-color="FF000000">
    <View width="110" height="380" flex-direction="column" flex-align-content="center"
          flex-justify-content="" flex-align-items="" repaint-hz="" margin="0"
          padding="0" border="0" background-color="00000000" id="shape">
      <View background-color="00000000" border="0" padding="0" margin="0"
            width="110" height="200"/>
      <Slider slider-type="rotary" slider-textbox="no-textbox" flex-align-self="center"
              height="100" lookAndFeel="Skeuomorphic" width="100" rotary-outline="FFE7EDFF"
              rotary-fill="ffbb4430" tooltip="Adds extra ambience to the chorus."
              id="SHAPE" background-color="00000000" padding="0" margin="4"
              parameter="blur" flex-grow="1" flex-shrink="1" caption=""/>
      <View background-color="00000000" border="0" padding="0" margin="0"
            width="110" height="135"/>
    </View>
    <View width="115" height="380" flex-direction="column" flex-align-content="center"
          flex-justify-content="" flex-align-items="" repaint-hz="" margin="0"
          padding="0" border="0" background-color="00000000" id="rate">
      <View background-color="00000000" border="0" padding="0" margin="0"
            width="110" height="170"/>
      <Slider slider-type="rotary" slider-textbox="no-textbox" flex-align-self="center"
              height="110" lookAndFeel="Skeuomorphic" width="110" rotary-fill="ffbb4430"
              tooltip="Increases the LFO speed." id="RATE" background-color="00000000"
              padding="0" margin="4" parameter="rate" caption=""/>
      <View background-color="00000000" border="0" padding="0" margin="0"
            width="110" height="165"/>
    </View>
    <View width="150" height="380" flex-direction="column" flex-align-content="center"
          flex-justify-content="" flex-align-items="" repaint-hz="" margin="0"
          padding="0" border="0" background-color="00000000" id="intensity">
      <View background-color="00000000" border="0" padding="0" margin="0"
            width="150" height="90"/>
      <Slider slider-type="rotary" slider-textbox="no-textbox" flex-align-self="center"
              height="120" parameter="intensity" lookAndFeel="Skeuomorphic"
              radius="" rotary-fill="ff99C24D" width="120" background-color=""
              tooltip="Increases the overall intensity of the effect." id="INTENSITY"
              padding="0" margin="4" caption="" rotary-outline="" slider-text=""
              slider-thumb="" slider-track=""/>
      <View background-color="00000000" border="0" padding="0" margin="0"
            width="150" height="210"/>
    </View>
    <View width="115" height="380" flex-direction="column" flex-align-content="center"
          flex-justify-content="" flex-align-items="" repaint-hz="" margin="0"
          padding="0" border="0" background-color="00000000" id="mix">
      <View background-color="00000000" border="0" padding="0" margin="0"
            width="110" height="170"/>
      <Slider slider-type="rotary" slider-textbox="no-textbox" flex-align-self="center"
              height="110" lookAndFeel="Skeuomorphic" width="110" rotary-fill="FFD32D56"
              tooltip="Increase the quantity of side signal. When the knob is set to half, the side is unchanged."
              id="MIX" background-color="00000000" padding="0" margin="4" rotary-outline="FF269138"
              slider-track="FF5633FF" slider-thumb="FFFFBC42" parameter="enhance"
              caption=""/>
      <View background-color="00000000" border="0" padding="0" margin="0"
            width="110" height="165"/>
    </View>
    <View width="110" height="380" flex-direction="column" flex-align-content="center"
          flex-justify-content="" flex-align-items="" repaint-hz="" margin="0"
          padding="0" border="0" background-color="00000000" id="enhance">
      <View background-color="00000000" border="0" padding="0" margin="0"
            width="110" height="200"/>
      <Slider slider-type="rotary" slider-textbox="no-textbox" flex-align-self="center"
              height="100" lookAndFeel="Skeuomorphic" width="100" rotary-fill="ffbb4430"
              tooltip="Controls the dry/wet ratio of the signal." id="ENHANCE"
              background-color="00000000" padding="0" margin="4" parameter="mix"
              caption=""/>
      <View background-color="00000000" border="0" padding="0" margin="0"
            width="110" height="135"/>
    </View>
  </View>
</magic>
